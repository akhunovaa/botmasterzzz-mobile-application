<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/list_view_item_horizontal_tab"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/channel_available_title_ghz_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/list_view_item_horizontal_spacer"
            android:textStyle="bold"/>

        <TextView
            android:id="@+id/channel_available_ghz_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/list_view_item_horizontal_tab"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/channel_available_title_ghz_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/list_view_item_horizontal_spacer"
            android:textStyle="bold"/>

        <TextView
            android:id="@+id/channel_available_ghz_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

</LinearLayout>
